@{
}


<table>
    <tr>
      
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Attend</th>
        @*<th>Details</th>*@
     

    </tr>
    @foreach (var gst in Model)
    {
        <tr>
          
            <td>@gst.Name</td>
            <td>@gst.Email</td>
            <td>@gst.Phone</td>
            <td>@gst.Attend</td>
            @*<th><a href=""><i class="fa-solid fa-eye"></i></a></th>*@

        </tr>
    }
</table>

